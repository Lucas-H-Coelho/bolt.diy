{
  "name": "bolt-diy",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "dev:backend": "nodemon --exec ts-node backend/src/server.ts",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start:backend": "node backend/dist/server.js",
    "test:backend": "jest --config backend/jest.config.js",
    "migration:generate:backend": "typeorm migration:generate -d backend/src/config/database.config.ts",
    "migration:run:backend": "typeorm migration:run -d backend/src/config/database.config.ts",
    "migration:revert:backend": "typeorm migration:revert -d backend/src/config/database.config.ts",
    "seed:run:backend": "ts-node backend/src/core/database/seeds/index.ts",
    "docker:run:backend": "docker-compose -f docker-compose-backend.yml up -d",
    "docker:stop:backend": "docker-compose -f docker-compose-backend.yml down",
    "docker:logs:backend": "docker-compose -f docker-compose-backend.yml logs -f",
    "setup:backend": "chmod +x backend/scripts/setup.sh && ./backend/scripts/setup.sh"
  },
